    <script>
      $(document).ready(function () {
        get_products();
      });
      function get_products() {
        $.ajax({
          type: 'GET',
          url: '/products',
          data: {},
          success: function (response) {
            // let doc = JSON.parse(response);
            console.log(response);
            for (let e of response.data) {
              let title = e.productName;
              let content = e.productExp;
              let questionNum = e.price;
              let temp_html = `<div>${questionNum}, ${title}, ${content}</div>`;
              $('.question__list').append(temp_html);
            }
            // for (let i = 0; i < response.length; i++) {
            //   let title = response[i].productName;
            //   let content = response[i].productExp;
            //   let questionNum = response[i].price;
            //   let temp_html = `<div>${questionNum}, ${title}, ${content}</div>`;
            //   $(".question__list").append(temp_html);
            //   console.log(temp_html)
            // }
          },
        });
      }
    </script>

    <title>공구해조</title>
